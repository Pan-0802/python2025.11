#约瑟夫环
n=eval(input('输入人数'))
k=eval(input('输入序号'))
lb = []
count=0
index=0
for i in range(1,n+1):
    lb.append(i)
while len(lb)>1:
            count+=1
            if count==k:
                print(lb[index])
                x=lb[index]
                lb.remove(x)
                count=0
            else:
                index=(index+1)%len(lb)
print(f'{lb[0]}')
